<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.gere0018.midterm" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <icon density="mdpi" src="www/img/people-48px.png" />
    <icon density="hdpi" src="www/img/people-72px.png" />
    <icon density="xhdpi" src="www/img/people-96px.png" />
    <icon density="xxhdpi" src="www/img/people-144px.png" />
    <icon density="xxhdpi" src="www/img/people-192px.png" />
    <name>Midterm-gere0018</name>
    <description>
       mideterm for mad9022 using cordov android platform contacts and geolocation api and hammer js.
    </description>
    <author email="gere0018@algonquinlive.com" href="https://github.com/gere0018/midterm-mad9022">
       Nehmat Gereige
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="android-minSdkVersion" value="18" />
</widget>
